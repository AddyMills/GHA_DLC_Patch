bonus_styles = [
	{
		id = guitarist_axel_outfit1_style1
		price = 250
		profile_name = 'Axel'
		info_name = axel
		blurb = "Axel peut retenir sa respiration pendant une minute et vingt-sept secondes.\\n\\nUn peu plus et il tombe par terre."
	}
	{
		id = guitarist_axel_outfit1_style2
		price = 250
		profile_name = 'Axel'
		info_name = axel
		blurb = "Axel est réputé pour ne jamais porter deux fois la même tenue. C'est un vrai créateur de mode, il jette sa tenue de scène après chaque concert. Il a même été connu pour jeter ses sous-vêtements au public.\\n\\n Ce look, c'est de l'Axel tout craché."
	}
	{
		id = guitarist_axel_outfit1_style3
		price = 250
		profile_name = 'Axel'
		info_name = axel
		blurb = "C'est en 1997 que l'oncle Bunch d'Axel a découvert cet entrepôt en Alabama. Ce dernier contenait des caisses remplies de jeans délavés neufs. \\n\\n Toujours sur le coup, Bunch a acheté toute la marchandise pour une bouchée de pain. \\n\\n Axel dispose désormais d'un stock important de cette icône de la mode des années 1980."
	}
	{
		id = guitarist_axel_outfit1_style4
		price = 250
		profile_name = 'Axel'
		info_name = axel
		blurb = "Il arrive parfois à Axel de se réveiller en pleine nuit en hurlant à cause d'un cauchemar récurrent dans lequel les stocks de jeans mondiaux sont épuisés."
	}
	{
		id = guitarist_axel_outfit1_style6
		price = 250
		profile_name = 'Axel'
		info_name = axel
		blurb = "Était-ce la destinée ou un coup de chance lorsqu'Axel a dessiné ce vêtement à l'alliance parfaite de peaux animales et de végétaux ? Lorsqu'on lui parle de sa prestance vestimentaire, Axel ne dit rien mais laisse glisser un clin d'oeil et un léger sourire en coin."
	}
	{
		id = guitarist_axel_outfit1_style7
		price = 250
		profile_name = 'Axel'
		info_name = axel
		blurb = "Où trouver des chaussures rouges comme les tiennes, Axel ? De préférence avec ces petits ornements blancs dessus... Tu peux faire demi-tour, Axel a pris la dernière paire. Axel sait reconnaître une bonne affaire quand il en voit une."
	}
	{
		id = guitarist_axel_outfit2_style1
		price = 250
		profile_name = 'Axel'
		info_name = axel
		blurb = "Lors de la phase d'introspection du groupe, concernant un changement de leur rythme de vie, Axel s'est laissé pousser la barbe et s'est mis à porter un chapeau. Lorsqu'il a été interviewé par le journaliste rock, mondialement reconnu, Vance Kristoffersen, Axel a déclaré qu'il avait été inspiré par 'les compositeurs de musique classique de 1800, ces hommes barbus aux cheveux longs'. Il a également déclaré vouloir 'donner en retour à la société'. D'après certaines sources, Vance se serait alors levé et aurait stoppé l'interview. Récemment Vance a déclaré : 'Je ne comprends pas un mot de ce que dit Axel Steel'."
	}
	{
		id = guitarist_axel_outfit2_style2
		price = 250
		profile_name = 'Axel'
		info_name = axel
		blurb = "Le monde du rock a subi un profond bouleversement lorsqu'Axel Steel a rasé son imposante barbe, celle-là même qu'il avait utilisée sur les deux derniers albums du groupe. Lors d'une interview avec le légendaire journaliste de rock Bunny 'John' Knutsenthings, Axel avait déclaré 'une grosse moustache est la véritable marque d'un homme vraiment bien, et c'est la manière dont j'aimerais qu'on se souvienne de moi. Vous pouvez emmener ça à la banque. Et si on vous demande, dites que vous ne savez pas pourquoi vous êtes à la banque'."
	}
	{
		id = guitarist_axel_outfit2_style3
		price = 250
		profile_name = 'Axel'
		info_name = axel
		blurb = "Pour le nouvel album, Axel s'est laissé pousser la barbe une nouvelle fois et l'a teinte en orange. Il s'est aussi mis à porter du blanc : 'C'est une marque de respect envers l'énorme barbe que j'avais il y a fort longtemps. Et puis surtout, les pellicules se voient moins'.\\n\\n Souviens-toi de ça."
	}
	{
		id = guitarist_axel_outfit2_style4
		price = 250
		profile_name = 'Axel'
		info_name = axel
		blurb = "Axel a adopté ce look lorsque le groupe a commencé sa tournée mondiale de l'Oklahoma. L'année prochaine le groupe prévoit de faire une tournée mondiale au Delaware."
	}
	{
		id = guitarist_axel_outfit2_style6
		price = 250
		profile_name = 'Axel'
		info_name = axel
		blurb = "Que peut-on dire de plus sur l'incroyable garde-robe d'Axel ? Pas grand chose, à en juger ce modèle."
	}
	{
		id = guitarist_axel_outfit2_style7
