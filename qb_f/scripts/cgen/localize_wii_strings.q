script get_string_wii \{message = unknown}
	localized_string = ""
	switch <message>
		case loading
		if german
			localized_string = $wii_special_german_9
		else
			localized_string = "Chargement des données en cours. Ne touchez pas au bouton POWER ni au bouton RESET."
		endif
		case saving
		if german
			localized_string = $wii_special_german_10
		else
			localized_string = "Sauvegarde en cours. Ne touchez pas au bouton POWER ni au bouton RESET."
		endif
		case autosaving
		if german
			localized_string = $wii_special_german_10
		else
			localized_string = "Sauvegarde en cours. Ne touchez pas au bouton POWER ni au bouton RESET."
		endif
		case overwriting
		if german
			localized_string = $wii_special_german_10
		else
			localized_string = "Sauvegarde en cours. Ne touchez pas au bouton POWER ni au bouton RESET."
		endif
		case formatting
		localized_string = "formatting"
		case load_successful
		localized_string = "Chargement réussi."
		case format_successful
		localized_string = "Formatage réussi."
		case save_successful
		localized_string = "Sauvegarde réussie."
		case overwrite_successful
		localized_string = "Écrasement réussi."
		case boot_autosave_warning
		localized_string = "boot_autosave_warning"
		case check_memcard
		if german
			localized_string = $wii_special_german_11
		else
			localized_string = "Vérification des données de sauvegarde.  Ne touchez pas au bouton POWER ni au bouton RESET."
		endif
		case no_memcard_boot
		localized_string = "no_memcard_boot"
		case no_memcard_save_boot
		localized_string = "no_memcard_save_boot"
		case no_save_boot
		localized_string = "Aucune sauvegarde Guitar Hero Aerosmith trouvée. Guitar Hero Aerosmith dispose d'un système d'Auto-sauvegarde. La sauvegarde des données de la partie nécessite 3 blocs d'espace libre.\\n\\n Voulez-vous créer un fichier de sauvegarde ?"
		case no_save_ingame
		localized_string = "Aucune sauvegarde Guitar Hero Aerosmith trouvée."
		case save_failed
		localized_string = "La sauvegarde a échoué ! Impossible de lire ou d'écrire dans la mémoire de la console Wii.\\nVeuillez vous référer au mode d'emploi Wii pour plus de détails."
		case overwrite_failed
		localized_string = "L'écrasement a échoué ! Impossible de lire ou d'écrire dans la mémoire de la console Wii.\\nVeuillez vous référer au mode d'emploi Wii pour plus de détails."
		case attempt_format
		localized_string = "attempt_format"
		case confirm_format
		localized_string = "confirm_format"
		case already_formatted
		localized_string = "already_formatted"
		case format_failed
		localized_string = "format_failed"
		case format_cancelled
		localized_string = "format_cancelled"
		case no_memcard_ingame
		localized_string = "no_memcard_ingame"
		case load_failed
		localized_string = "Le chargement a échoué ! Impossible de lire ou d'écrire dans la mémoire de la console Wii.\\nVeuillez vous référer au mode d'emploi Wii pour plus de détails."
		case overwrite_warning_boot
		localized_string = "overwrite_warning_boot"
		case disable_autosave
		localized_string = "Le système d'Auto-sauvegarde est désactivé.\\nLe système d'Auto-sauvegarde peut être activé depuis le menu Options."
		case overwrite_warning_ingame
		localized_string = "ATTENTION : une sauvegarde écrasera les données Guitar Hero Aerosmith existantes. Sauvegarder ?"
		case lose_progress_ingame
		localized_string = "ATTENTION : si vous chargez des données, vous perdrez votre\\nprogression actuelle. Charger ?"
		case insufficient_space
		localized_string = "Il n'y a pas assez d'espace libre dans la mémoire de la console Wii.\\nLibérez 3 blocs en transférant des données vers une carte SD ou en les effaçant depuis l'écran de gestion des données.\\nVous ne pourrez accéder aux données de la connexion Wi-Fi Nintendo qu'une fois cela fait."
		case insufficient_space_wfc_only
		localized_string = "Il n'y a pas assez d'espace libre dans la mémoire de la console Wii pour sauvegarder \\nles données de connexion Wi-Fi Nintendo. Libérez 1 bloc en transférant des données vers une carte SD\\n ou en les effaçant depuis l'écran de gestion des données. Les données existantes peuvent être chargées et écrasées.\\nVous ne pourrez accéder aux données de la connexion Wi-Fi Nintendo qu'une fois cela fait."
		case insufficient_inode
		localized_string = "Il n'y a pas assez d'espace libre dans la mémoire de la console Wii.\\nTransférez des données sur une carte SD ou effacez des données depuis l'écran de gestion des données.\\nVous ne pourrez accéder aux données de la connexion Wi-Fi Nintendo qu'une fois cela fait."
		case insufficient_inode_wfc_only
		localized_string = "Il n'y a pas assez d'espace libre dans la mémoire de la console Wii pour sauvegarder \\nles données de connexion Wi-Fi Nintendo. Transférez des données sur une carte SD ou effacez des données depuis l'écran de gestion des données.\\nLes données existantes peuvent être chargées et écrasées.\\nVous ne pourrez accéder aux données de la connexion Wi-Fi Nintendo qu'une fois cela fait."
		case corrupt_boot
		localized_string = "Le fichier Guitar Hero Aerosmith contient des données corrompues et ne peut pas être utilisé."
		case confirm_overwrite_boot
		localized_string = "Confirmer l'écrasement de ce fichier ?"
		case new_memcard_ingame
		localized_string = "new_memcard_ingame"
		case warning_480p
		localized_string = "Le mode Progressif peut ne pas fonctionner sur tous les téléviseurs. S'il ne fonctionne pas sur le tien, attends 15 secondes et nous le ferons basculer automatiquement sur le mode précédent."
		case now_in_480p
		localized_string = "Tu es maintenant en mode progressif.  Veux-tu conserver ce mode d'affichage ?"
		default
		printf "No match, string %d not localized" d = <message>
		localized_string = "???"
	endswitch
	return localized_string = <localized_string>
endscript
