script get_string_wii \{message = unknown}
	localized_string = ""
	switch <message>
		case loading
		if german
			localized_string = $wii_special_german_9
		else
			localized_string = "Caricamento in corso... Non resettare né spegnere la console."
		endif
		case saving
		if german
			localized_string = $wii_special_german_10
		else
			localized_string = "Salvataggio in corso... Non resettare né spegnere la console."
		endif
		case autosaving
		if german
			localized_string = $wii_special_german_10
		else
			localized_string = "Salvataggio in corso... Non resettare né spegnere la console."
		endif
		case overwriting
		if german
			localized_string = $wii_special_german_10
		else
			localized_string = "Salvataggio in corso... Non resettare né spegnere la console."
		endif
		case formatting
		localized_string = "formatting"
		case load_successful
		localized_string = "Caricamento completato."
		case format_successful
		localized_string = "Formattazione completata."
		case save_successful
		localized_string = "Salvataggio completato."
		case overwrite_successful
		localized_string = "Sovrascrittura completata."
		case boot_autosave_warning
		localized_string = "boot_autosave_warning"
		case check_memcard
		if german
			localized_string = $wii_special_german_11
		else
			localized_string = "Controllo dei dati salvati in corso. Non toccare il pulsante POWER o il pulsante RESET."
		endif
		case no_memcard_boot
		localized_string = "no_memcard_boot"
		case no_memcard_save_boot
		localized_string = "no_memcard_save_boot"
		case no_save_boot
		localized_string = "Nessun dato salvato di Guitar Hero Aerosmith rilevato.  Guitar Hero Aerosmith utilizza una funzione di Autosalvataggio. Sono richiesti 3 blocchi di memoria per salvare i dati di gioco.\\n\\n Creare un nuovo file di salvataggio?"
		case no_save_ingame
		localized_string = "Nessun dato salvato di Guitar Hero Aerosmith rilevato. "
		case save_failed
		localized_string = "Salvataggio non riuscito! Impossibile leggere o modificare i dati all'interno della memoria della console Wii.\\nPer maggiori informazioni, consulta il manuale di istruzioni della console Wii."
		case overwrite_failed
		localized_string = "Sovrascrittura non riuscita! Impossibile leggere o modificare i dati all'interno della memoria della console Wii.\\nPer maggiori informazioni, consulta il manuale di istruzioni della console Wii."
		case attempt_format
		localized_string = "attempt_format"
		case confirm_format
		localized_string = "confirm_format"
		case already_formatted
		localized_string = "already_formatted"
		case format_failed
		localized_string = "format_failed"
		case format_cancelled
		localized_string = "format_cancelled"
		case no_memcard_ingame
		localized_string = "no_memcard_ingame"
		case load_failed
		localized_string = "Caricamento non riuscito! Impossibile leggere o modificare i dati all'interno della memoria della console Wii. Per maggiori informazioni, consulta il manuale di istruzioni della console Wii."
		case overwrite_warning_boot
		localized_string = "overwrite_warning_boot"
		case disable_autosave
		localized_string = "Il salvataggio automatico è stato disattivato.\\nIl salvataggio automatico può essere riattivato dal menu delle opzioni."
		case overwrite_warning_ingame
		localized_string = "ATTENZIONE: il salvataggio comporterà la sovrascrittura dei dati esistenti di Guitar Hero Aerosmith. Vuoi continuare?"
		case lose_progress_ingame
		localized_string = "ATTENZIONE: se si caricano dati, i progressi di gioco\\ncorrenti andranno persi. Continuare il caricamento?"
		case insufficient_space
		localized_string = "Non c'è abbastanza spazio libero nella memoria della console Wii.\\nBlocchi liberi necessari: 3. Per creare spazio, vai allo schermo Gestione dati e cancella alcuni dati o trasferiscili in una scheda SD.\\nIn seguito sarai in grado di accedere alla Nintendo WFC."
		case insufficient_space_wfc_only
		localized_string = "La memoria di sistema disponibile della console Wii è insufficiente per salvare i dati della \\nNintendo WFC. Blocchi liberi necessari: 1. Per creare spazio, vai allo schermo Gestione dati e cancella alcuni dati o\\n trasferiscili in una scheda SD. È possibile caricare e sovrascrivere i dati già esistenti.\\nIn seguito sarai in grado di accedere alla Nintendo WFC. "
		case insufficient_inode
		localized_string = "Non c'è abbastanza spazio libero nella memoria della console Wii.\\nPer creare spazio, vai allo schermo Gestione dati e cancella alcuni dati o trasferiscili in una scheda SD.\\nIn seguito sarai in grado di accedere alla Nintendo WFC."
		case insufficient_inode_wfc_only
		localized_string = "La memoria di sistema disponibile della console Wii è insufficiente per salvare i dati della \\nNintendo WFC. Per creare spazio, vai allo schermo Gestione dati e cancella alcuni dati o trasferiscili in una scheda SD.\\nÈ possibile caricare e sovrascrivere i dati già esistenti.\\nIn seguito sarai in grado di accedere alla Nintendo WFC."
		case corrupt_boot
		localized_string = "Il file di Guitar Hero Aerosmith non può essere usato perché i dati sono danneggiati.\\n"
		case confirm_overwrite_boot
		localized_string = "Sovrascrivere questo file?"
		case new_memcard_ingame
		localized_string = "new_memcard_ingame"
		case warning_480p
		localized_string = "La modalità progressiva potrebbe non funzionare con tutti i televisori.  Se non funziona, attendi 15 secondi e si annullerà automaticamente."
		case now_in_480p
		localized_string = "Adesso sei in modalità progressiva. Vuoi tenere questa modalità di visualizzazione?"
		default
		printf "No match, string %d not localized" d = <message>
		localized_string = "???"
	endswitch
	return localized_string = <localized_string>
endscript
