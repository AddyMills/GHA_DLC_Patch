	{
		time = 44.5
		duration = 3.5
		text = "e anche lui veniva a Sunapee, da suo zio."
	}
	{
		time = 48.0
		duration = 3.0
		text = "E ogni tanto veniva e noi andavamo a sentire la sua band"
	}
	{
		time = 51.0
		duration = 2.0
		text = "(Tom Hamilton) Soprattutto in un posto che si chiamava The Barn."
	}
	{
		time = 53.0
		duration = 1.5
		text = "Era un club dove"
	}
