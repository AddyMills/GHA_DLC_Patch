		time = 54.5
		duration = 1.5
		text = "le mamme non volevano lasciar andare i figli."
	}
	{
		time = 56.5
		duration = 4.0
		text = "(Joe Perry) Era alla ricerca di una nuova esperienza e alla fine"
	}
	{
		time = 60.5
		duration = 3.0
		text = " visto suonare Tom insieme a me, e ha visto che eravamo solamente"
