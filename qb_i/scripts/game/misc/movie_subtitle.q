	{
		time = 32.0
		duration = 4.0
		text = "E Tom abitava proprio lì."
	}
	{
		time = 35.0
		duration = 3.0
		text = "E ogni estate ci trovavamo e passavamo il tempo insieme."
	}
	{
		time = 38.0
		duration = 2.0
		text = "Ci piaceva lo stesso genere di musica."
	}
	{
