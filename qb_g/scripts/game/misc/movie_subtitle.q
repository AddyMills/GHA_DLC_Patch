	{
		time = 172.0
		duration = 2.0
		text = "Nipmuc war auch einer."
	}
	{
		time = 174.0
		duration = 3.0
		text = "(Tom Hamilton) Wir spielten in der Turnhalle bei einem Highschool-Ball."
	}
	{
		time = 177.0
		duration = 3.0
		text = "(Steven Tyler) Zu der Zeit merkten wir, dass sich die Dinge entwickelten."
	}
	{
		time = 180.0
		duration = 3.0
		text = "Es war magisch, dass wir nicht nur"
