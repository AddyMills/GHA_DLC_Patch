	{
		time = 209.5
		duration = 1.0
		text = "wir hatten nichts, aber"
	}
	{
		time = 211.0
		duration = 1.0
		text = "ich wollte unbedingt etwas mit nach Hause nehmen."
	}
	{
		time = 212.0
		duration = 3.5
		text = "ich ging in den Umkleideraum, in dem wir uns umziehen konnten."
	}
	{
		time = 215.5
		duration = 5.0
		text = "(Joey Kramer) Wir haben T-Shirts aus der Turnhalle der Schule geklaut."
	}
	{
