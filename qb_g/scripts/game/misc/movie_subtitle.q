	}
	{
		time = 186.0
		duration = 3.0
		text = "Ich bin mir zwar nicht sicher, was das war, aber"
	}
	{
		time = 189.0
		duration = 3.0
		text = "wir haben gelernt, wie wir eine Band werden, zusammen spielen"
