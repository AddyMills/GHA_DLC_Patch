bonus_styles = [
	{
		id = guitarist_axel_outfit1_style1
		price = 250
		profile_name = 'Axel'
		info_name = axel
		blurb = "Axel kann eine Minute und 27 Sekunden lang die Luft anhalten\\n\\nWenn er sie länger anhält, fällt er um."
	}
	{
		id = guitarist_axel_outfit1_style2
		price = 250
		profile_name = 'Axel'
		info_name = axel
		blurb = "Axel ist dafür bekannt, niemals das gleiche Outfit zweimal zu tragen. Er wirft seine Bühnenkleidung einfach nach einem einzigen Auftritt weg. Er ist sogar dafür bekannt, seine Unterwäsche ins Publikum zu werfen.\\n\\n Dieser Look ist Axels eigene Schöpfung."
	}
	{
		id = guitarist_axel_outfit1_style3
		price = 250
		profile_name = 'Axel'
		info_name = axel
		blurb = "Im Jahr 1987 entdeckte Axels Onkel Bunch ein Lagerhaus in Alabama, in dem sich ungeöffnete Kisten voller Stonewashed Jeans befanden. \\n\\n Ganz der Zocker, kaufte Butch das ganze Lager für einen Spottpreis auf. \\n\\n Jetzt hat Axel einen konstanten Nachschub dieser legendären Kleidungsstücke aus den Achtzigern."
	}
	{
		id = guitarist_axel_outfit1_style4
		price = 250
		profile_name = 'Axel'
		info_name = axel
		blurb = "Manchmal wacht Axel nachts schreiend auf - er hat den immer wiederkehrenden Traum, dass der Weltvorrat an Jeansstoff noch zu seinen Lebzeiten ausgeht."
	}
	{
		id = guitarist_axel_outfit1_style6
		price = 250
		profile_name = 'Axel'
		info_name = axel
		blurb = "War es Schicksal oder Glück, dass Axel auf diese perfekte Kombination von Tierhäuten und Gemüse brachte? Axel sagt nichts dazu, zwinkert nur und schenkt uns ein leichtes Grinsen, wenn wir ihn zu seinem ausdrucksstarken Kleidungsstil befragen."
	}
	{
		id = guitarist_axel_outfit1_style7
		price = 250
		profile_name = 'Axel'
		info_name = axel
		blurb = "Wo kann ich so ein Paar roter Schuhe wie deine herkriegen, Axel?  Vorzugsweise mit weißen Verzierungen... Wie sich herausstellte, hat Axel das letzte Paar erwischt. Axel erkennt ein gutes Geschäft, wenn er es sieht."
	}
	{
		id = guitarist_axel_outfit2_style1
		price = 250
		profile_name = 'Axel'
		info_name = axel
		blurb = "Während der introspektiven Phase der Band ließ sich Axel einen Bart wachsen und begann, einen Hut zu tragen. Als er von dem international berühmten Rockjournalisten Vance Kristoffersen interviewt wurde, merkte Axel an, dass er 'von den langhaarigen, bärtigen Komponisten klassicher Musik aus dem 19ten Jahrhundert inspiriert wurde' und 'der Gesellschafft twas zurückgeben will'. Berichten zufolge stand Vince während des Interviews auf und ging einfach. Vor kurzem erklärte Vance: 'Ich versteh kein Wort von dem, was Axel sagt.'"
	}
	{
		id = guitarist_axel_outfit2_style2
		price = 250
		profile_name = 'Axel'
		info_name = axel
		blurb = "Die Rockwelt war in ihren Grundfesten erschüttert, als Axel Steel seinen mächtigen Bart abrasierte, den er bei den letzten zwei Alben der Band zur Schau gestellt hatte. In einem Interview mit dem legendären Rockjournalisten Bunny 'John' Knutsenthings bemerkte Axel, dass 'ein großer Schnurrbart das Markenzeichen eines aufrechten Mannes ist und als solchen soll man sich an mich erinnern. Das kannst du auf die Bank bringen. Und wenn dich jemand fragt, sag, dass du keine Ahnung hast, warum du in der Bank bist.'"
	}
	{
		id = guitarist_axel_outfit2_style3
		price = 250
		profile_name = 'Axel'
		info_name = axel
		blurb = "Für das neue Album ließ Axel seinen Bart wieder wachsen und färbte ihn orange. Er ging auch dazu über, Weiß zu tragen 'als Zeichen des Respekts für den großen Bart, den ich vor einiger Zeit abrasiert habe. Ausserdem sieht man Schuppen nicht so deutlich.' Wahrlich weise Worte."
	}
	{
		id = guitarist_axel_outfit2_style4
		price = 250
		profile_name = 'Axel'
		info_name = axel
		blurb = "Axel übernahm diesen Look für die World Series Tour seiner Band durch Oklahoma. Nächstes Jahr plant die Band eine World Series Tour durch Delaware."
	}
	{
		id = guitarist_axel_outfit2_style6
		price = 250
		profile_name = 'Axel'
		info_name = axel
		blurb = "Was lässt sich noch über Axels beeindruckenden Kleidungsstil sagen? Nicht mehr viel, wenn man sich dieses Outfit anschaut."
	}
	{
		id = guitarist_axel_outfit2_style7
		price = 250
