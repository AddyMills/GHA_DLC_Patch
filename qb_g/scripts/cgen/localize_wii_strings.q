script get_string_wii \{message = unknown}
	localized_string = ""
	switch <message>
		case loading
		if german
			localized_string = $wii_special_german_9
		else
			localized_string = "Lädt.  Bitte berühre weder den POWER-Schalter noch den RESET-Knopf."
		endif
		case saving
		if german
			localized_string = $wii_special_german_10
		else
			localized_string = "Speichern.  Bitte berühre weder den POWER-Schalter noch den RESET-Knopf."
		endif
		case autosaving
		if german
			localized_string = $wii_special_german_10
		else
			localized_string = "Speichern.  Bitte berühre weder den POWER-Schalter noch den RESET-Knopf."
		endif
		case overwriting
		if german
			localized_string = $wii_special_german_10
		else
			localized_string = "Speichern.  Bitte berühre weder den POWER-Schalter noch den RESET-Knopf."
		endif
		case formatting
		localized_string = "formatting"
		case load_successful
		localized_string = "Laden erfolgreich."
		case format_successful
		localized_string = "Formatierung erfolgreich."
		case save_successful
		localized_string = "Speichern erfolgreich."
		case overwrite_successful
		localized_string = "Überschreiben erfolgreich."
		case boot_autosave_warning
		localized_string = "boot_autosave_warning"
		case check_memcard
		if german
			localized_string = $wii_special_german_11
		else
			localized_string = "Speicherdaten werden überprüft.  Bitte berühre weder den POWER-Schalter noch den RESET-Knopf."
		endif
		case no_memcard_boot
		localized_string = "no_memcard_boot"
		case no_memcard_save_boot
		localized_string = "no_memcard_save_boot"
		case no_save_boot
		localized_string = "Keine Guitar Hero Aerosmith-Daten vorhanden. Guitar Hero Aerosmith speichert automatisch. 3 freie Blöcke werden zum Speichern von Spieldaten benötigt. Möchtest du nun eine Datei erstellen?"
		case no_save_ingame
		localized_string = "Keine Guitar Hero Aerosmith-Daten vorhanden."
		case save_failed
		localized_string = "Fehler beim Laden! Der Speicher der Wii-Konsole konnte nicht überschrieben/gelesen werden.\\nBitte lies die Wii-Bedienungsanleitung, um weitere Informationen zu erhalten."
		case overwrite_failed
		localized_string = "Fehler beim Überschreiben! Der Speicher der Wii-Konsole konnte nicht überschrieben/gelesen werden.\\nBitte lies die Wii-Bedienungsanleitung, um weitere Informationen zu erhalten."
		case attempt_format
		localized_string = "attempt_format"
		case confirm_format
		localized_string = "confirm_format"
		case already_formatted
		localized_string = "already_formatted"
		case format_failed
		localized_string = "format_failed"
		case format_cancelled
		localized_string = "format_cancelled"
		case no_memcard_ingame
		localized_string = "no_memcard_ingame"
		case load_failed
		localized_string = "Laden fehlgeschlagen! Der Speicher der Wii-Konsole konnte nicht überschrieben/gelesen werden.\\nBitte lies die Wii-Bedienungsanleitung, um weitere Informationen zu erhalten."
		case overwrite_warning_boot
		localized_string = "overwrite_warning_boot"
		case disable_autosave
		localized_string = "Die Funktion zur automatischen Speicherung wurde deaktiviert. \\nIm Menü 'Optionen' können Sie die Funktion zur automatischen Speicherung wieder aktivieren."
		case overwrite_warning_ingame
		localized_string = "WARNUNG: Beim Speichern werden alle vorhandenen Guitar Hero: Aerosmith-Daten überschrieben. Möchtest du wirklich speichern?"
		case lose_progress_ingame
		localized_string = "WARNUNG: Wenn Du Daten ladst, geht Dein\\naktueller Spielstand verloren. Mit dem Laden fortfahren?"
		case insufficient_space
		localized_string = "Der Speicher der Wii-Konsole ist belegt. \\nBitte rufe den Datenverwaltungsbildschirm auf, um Daten auf eine SD Card zu verschieben oder zu löschen (erforderliche Blöcke: 3). Bis dies getan wurde, kannst du nicht auf WFC-Daten zugreifen."
		case insufficient_space_wfc_only
		localized_string = "Der Speicher der Wii-Konsole ist belegt, es können keine Nintendo WFC-Daten \\ngespeichert werden. Bitte rufe den Datenverwaltungsbildschirm auf, um Daten auf eine SD\\n Card zu verschieben oder zu löschen (erforderliche Blöcke: 1). Alle Daten können geladen und überschrieben werden. \\nBis du dies getan hast, kannst du keine Verbindung zur Nintendo WFC aufbauen."
		case insufficient_inode
		localized_string = "Es können keine weiteren Dateien im Speicher der Wii-Konsole gespeichert werden.\\nBitte rufe den Datenverwaltungsbildschirm auf, um Daten auf eine SD Card zu verschieben oder zu löschen.\\nBis dies getan wurde, kannst du nicht auf Nintendo WFC-Daten zugreifen."
		case insufficient_inode_wfc_only
		localized_string = "Der Speicher der Wii-Konsole ist belegt, es können keine Nintendo WFC-Daten \\ngespeichert werden. Bitte rufe den Datenverwaltungsbildschirm auf, um Daten auf eine SD Card zu verschieben oder zu löschen.\\nAlle Daten können geladen und überschrieben werden. Alle Daten können geladen und überschrieben werden.\\n Bis dies getan wurde, kannst du nicht auf WFC-Daten zugreifen."
		case corrupt_boot
		localized_string = "Die Guitar Hero: Aerosmith-Datei ist fehlerhaft und kann daher nicht verwendet werden."
		case confirm_overwrite_boot
		localized_string = "Sind Sie sicher, dass Sie diese Datei überschreiben möchten?"
		case new_memcard_ingame
		localized_string = "new_memcard_ingame"
		case warning_480p
		localized_string = "Der Progressive-Modus funktioniert nicht bei allen Fernsehgeräten.  Wenn er bei Deinem nicht geht, warte 15 Sekunden, bis automatisch zurückgeschaltet wird."
		case now_in_480p
		localized_string = "Du befindest Dich jetzt im Progressive-Modus.  Möchtest Du diesen Anzeigemodus beibehalten?"
		default
		printf "No match, string %d not localized" d = <message>
		localized_string = "???"
	endswitch
	return localized_string = <localized_string>
endscript
