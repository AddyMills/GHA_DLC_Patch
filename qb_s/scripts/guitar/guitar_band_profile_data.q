script get_musician_body_size 
	getarraysize \{musician_body
		globalarray}
	size = (<array_size>)
	if globalexists \{name = download_musician_body
			type = array}
		getarraysize \{download_musician_body
			globalarray}
		size = (<array_size> + <size>)
	endif
	return array_size = <size>
