		duration = 1.0
		text = "al menos quería llevarme algo a casa"
	}
	{
		time = 212.0
		duration = 3.5
		text = "así que fui a las taquillas donde nos habían dejado cambiarnos."
	}
	{
		time = 215.5
		duration = 5.0
		text = "(Joey Kramer) Robamos camisetas del gimnasio de aquel instituto."
	}
	{
		time = 220.5
		duration = 3.0
		text = "Steven y yo todavía las guardamos."
	}
	{
