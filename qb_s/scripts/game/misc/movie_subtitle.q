		duration = 3.0
		text = "que había magia, que ya no estábamos"
	}
	{
		time = 183.0
		duration = 3.0
		text = "solo ensayando, que habíamos descubierto el secreto."
	}
	{
		time = 186.0
		duration = 3.0
		text = "Y no estoy del todo seguro de qué es, pero"
	}
	{
		time = 189.0
		duration = 3.0
		text = "aprendimos a ser un grupo y a tocar juntos"
