script get_string_wii \{message = unknown}
	localized_string = ""
	switch <message>
		case loading
		if german
			localized_string = $wii_special_german_9
		else
			localized_string = "Cargando. No toques el Botón POWER ni el Botón RESET."
		endif
		case saving
		if german
			localized_string = $wii_special_german_10
		else
			localized_string = "Guardando. No toques el Botón POWER ni el Botón RESET."
		endif
		case autosaving
		if german
			localized_string = $wii_special_german_10
		else
			localized_string = "Guardando. No toques el Botón POWER ni el Botón RESET."
		endif
		case overwriting
		if german
			localized_string = $wii_special_german_10
		else
			localized_string = "Guardando. No toques el Botón POWER ni el Botón RESET."
		endif
		case formatting
		localized_string = "formatting"
		case load_successful
		localized_string = "Se ha cargado correctamente."
		case format_successful
		localized_string = "Éxito al formatear."
		case save_successful
		localized_string = "Éxito al guardar."
		case overwrite_successful
		localized_string = "Éxito al sobrescribir."
		case boot_autosave_warning
		localized_string = "boot_autosave_warning"
		case check_memcard
		if german
			localized_string = $wii_special_german_11
		else
			localized_string = "Comprobando datos guardados.  No toques el Botón POWER ni el Botón RESET."
		endif
		case no_memcard_boot
		localized_string = "no_memcard_boot"
		case no_memcard_save_boot
		localized_string = "no_memcard_save_boot"
		case no_save_boot
		localized_string = "No se encontraron datos de Guitar Hero Aerosmith. Guitar Hero Aerosmith utiliza una función de autoguardado. Se necesitan 3 bloques de espacio libre para guardar los datos de la partida.\\n\\n ¿Quieres crear un archivo para guardar datos?"
		case no_save_ingame
		localized_string = "No se encontraron datos de Guitar Hero Aerosmith."
		case save_failed
		localized_string = "¡Error al guardar! No se ha podido acceder a la memoria de la consola Wii.\\nConsulta el manual de instrucciones de la consola Wii para obtener más información."
		case overwrite_failed
		localized_string = "¡Error al sobrescribir! No se ha podido/acceder a la memoria de la consola Wii.\\nConsulta el manual de instrucciones de la consola Wii para obtener más información."
		case attempt_format
		localized_string = "attempt_format"
		case confirm_format
		localized_string = "confirm_format"
		case already_formatted
		localized_string = "already_formatted"
		case format_failed
		localized_string = "format_failed"
		case format_cancelled
		localized_string = "format_cancelled"
		case no_memcard_ingame
		localized_string = "no_memcard_ingame"
		case load_failed
		localized_string = "¡Error al cargar! No se ha podido acceder a la memoria de la consola Wii.\\nConsulta el manual de instrucciones de la consola Wii para obtener más información."
		case overwrite_warning_boot
		localized_string = "overwrite_warning_boot"
		case disable_autosave
		localized_string = "La función de autoguardado ha sido desactivada. \\nLa función de autoguardado puede reactivarse en el menú Opciones."
		case overwrite_warning_ingame
		localized_string = "ADVERTENCIA: si guardas sobrescribirás los datos de Guitar Hero Aerosmith existentes. ¿Seguro que quieres guardar?"
		case lose_progress_ingame
		localized_string = "ADVERTENCIA: si cargas datos, perderás el progreso\\nde tu partida actual. ¿Continuar cargando?"
		case insufficient_space
		localized_string = "No hay suficiente espacio libre en la memoria de la consola Wii.\\nBloques libres necesarios: 3. Transfiere datos a una tarjeta SD o borra los que no necesites en la pantalla de gestión de datos.\\nHasta que no lo hagas no podrás acceder a los datos de la CWF de Nintendo."
		case insufficient_space_wfc_only
		localized_string = "No hay suficiente espacio libre en la memoria de la consola Wii para guardar los\\n datos de la CWF de Nintendo. Bloques libres necesarios: 1. Transfiere datos a una tarjeta SD\\n o borra los que no necesites en la pantalla de gestión de datos. Todos los datos guardados pueden ser cargados y sobrescritos.\\nHasta que no lo hagas no podrás acceder a la CWF de Nintendo."
		case insufficient_inode
		localized_string = "No hay suficiente espacio libre en la memoria de la consola Wii.\\nTransfiere datos a una tarjeta SD o borra los que no necesites en la pantalla de gestión de datos.\\nHasta que no lo hagas no podrás acceder a los datos de la CWF de Nintendo."
		case insufficient_inode_wfc_only
		localized_string = "No hay suficiente espacio libre en la memoria de la consola Wii para guardar datos de la CWF de Nintendo.\\nTransfiere datos a una tarjeta SD o borra los que no necesites en la pantalla de gestión de datos.\\nTodos los datos guardados pueden ser cargados y sobrescritos.\\nHasta que no lo hagas no podrás acceder a los datos de la CWF de Nintendo."
		case corrupt_boot
		localized_string = "El archivo de Guitar Hero Aerosmith está dañado y no se puede usar.\\n"
		case confirm_overwrite_boot
		localized_string = "¿Estás seguro de querer sobrescribir el archivo de guardado?"
		case new_memcard_ingame
		localized_string = "new_memcard_ingame"
		case warning_480p
		localized_string = "El modo progresivo puede que no funcione en todas las televisiones. Si no funciona en tu televisor, espera 15 segundos y lo cambiaremos por ti automáticamente."
		case now_in_480p
		localized_string = "Ahora estás en modo progresivo.  ¿Quieres mantener este modo de visualización?"
		default
		printf "No match, string %d not localized" d = <message>
		localized_string = "???"
	endswitch
	return localized_string = <localized_string>
endscript
